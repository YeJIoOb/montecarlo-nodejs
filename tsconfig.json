{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es6",
    "noImplicitAny": false,
    "sourceMap": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "rootDir": "./src/",
    "outDir": "./build/"
  },
  "exclude": ["**/node_modules/*", "node-services"],
  "skipLibCheck": true
}
